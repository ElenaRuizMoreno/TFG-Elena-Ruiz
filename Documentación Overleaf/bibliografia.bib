@inproceedings{mohan2010blood,
  author    = {S. Mohan and A. Arora and T. D. Sauer and A. Sharma and R. K. Sharma and M. Srivastava},
  title     = {Blood Oxygen Estimation from Compressively Sensed Photoplethysmograph},
  booktitle = {Proceedings of the 8th ACM Conference on Embedded Networked Sensor Systems (SenSys '10)},
  year      = {2010},
  pages     = {183--196},
  publisher = {ACM},
  address   = {New York, NY, USA},
  doi       = {10.1145/1921081.1921084},
  url       = {https://dl.acm.org/doi/10.1145/1921081.1921084},
  abstract  = {Este artículo presenta un método para estimar la saturación de oxígeno en sangre (SpO₂) a partir de señales de fotopletismografía (PPG) adquiridas mediante muestreo compresivo. Se propone una plataforma de bajo consumo para sensores portátiles de oximetría de pulso, adecuada para aplicaciones de monitoreo remoto de signos vitales. Los autores demuestran que la precisión en la estimación de SpO₂ no se ve comprometida por la presencia de artefactos de luz ambiental, incluso cuando se utilizan tasas de muestreo reducidas.}
}

@article{shuzan2023machine,
  author    = {Md Nazmul Islam Shuzan and Moajjem Hossain Chowdhury and Muhammad E. H. Chowdhury and Murugappan Murugappan and Enamul Hoque Bhuiyan and Mohamed Arslane Ayari and Amith Khandakar},
  title     = {Machine Learning-Based Respiration Rate and Blood Oxygen Saturation Estimation Using Photoplethysmogram Signals},
  journal   = {Bioengineering},
  volume    = {10},
  number    = {2},
  pages     = {167},
  year      = {2023},
  doi       = {10.3390/bioengineering10020167},
  url       = {https://www.mdpi.com/2306-5354/10/2/167},
  abstract  = {Este estudio propone un método novedoso para estimar la frecuencia respiratoria (RR) y la saturación de oxígeno en sangre (SpO₂) utilizando modelos de aprendizaje automático que incorporan características extraídas de señales de fotopletismografía (PPG). Se emplearon técnicas de selección de características para reducir la complejidad computacional y evitar el sobreajuste. Se entrenaron 19 modelos de regresión para RR y SpO₂, seleccionando el modelo de regresión de procesos gaussianos como el más preciso. Los resultados mostraron errores absolutos medios (MAE) de 0.89 para RR y 0.57 para SpO₂, y errores cuadráticos medios (RMSE) de 1.41 para RR y 0.98 para SpO₂, indicando una estimación fiable y eficiente.}
}

@techreport{oak2015how,
  author      = {Sang-Soo Oak and Praveen Aroul},
  title       = {How to Design Peripheral Oxygen Saturation (SpO₂) and Optical Heart Rate Monitoring (OHRM) Systems Using AFE4403},
  institution = {Texas Instruments},
  number      = {SLAA655},
  year        = {2015},
  month       = {March},
  url         = {https://www.ti.com/lit/an/slaa655/slaa655.pdf},
  note        = {Application Report},
  abstract    = {Este informe técnico proporciona una guía detallada para diseñar sistemas de medición de saturación de oxígeno periférico (SpO₂) y monitoreo óptico de la frecuencia cardíaca (OHRM) utilizando el circuito integrado AFE4403. Se abordan aspectos clave como la adquisición de la señal de fotopletismografía (PPG), la configuración óptima de corriente de los LED y la ganancia del amplificador de transimpedancia (TIA), así como técnicas de calibración para adaptarse a variaciones individuales en la absorción de luz. Además, se discuten consideraciones sobre el procesamiento de señales y la mitigación de artefactos de movimiento para mejorar la precisión en aplicaciones clínicas y dispositivos portátiles.}
}

